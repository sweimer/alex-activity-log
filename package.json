{
  "name": "alex-activity-log",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm start --prefix frontend\" \"npm start --prefix backend\"",
    "stop": "concurrently \"pkill -f 'npm start --prefix frontend'\" \"pkill -f 'npm start --prefix backend'\"",
    "build": "concurrently \"npm run build --prefix frontend\" \"npm run build --prefix backend\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}